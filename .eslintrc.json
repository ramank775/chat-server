      "browser": true,
      "es6": true,
      "node": true
    },
    "plugins": ["prettier"],
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module"
    },
    "rules": {
      "prettier/prettier": "error"
    }
  }
